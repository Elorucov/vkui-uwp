                        <Button Margin="0,6,6,0" Style="{StaticResource VKButtonTertiaryExtraLarge}" Content="Tertiary"/>
                        <Button Margin="0,6,6,0" Style="{StaticResource VKButtonOutlineExtraLarge}" Content="Outline"/>
                        <Button Margin="0,6,6,0" Style="{StaticResource VKButtonCommerceExtraLarge}" Content="Commerce"/>
